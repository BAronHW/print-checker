{
  "name": "print_check",
  "type": "module",
  "version": "1.1.2",
  "description": "A Git pre-commit hook that detects and prevents print/debug statements from being committed",
  "main": "dist/print_detect_index.js",
  "bin": {
    "print_check": "dist/print_detect_index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "!dist/tests/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node dist/print_detect_index.js",
    "test": "node --test",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "dev": "node --watch print_detect_index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "git",
    "pre-commit",
    "hook",
    "print",
    "console.log",
    "debug",
    "linter",
    "commit",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "chalk": "^5.6.2"
  }
}
